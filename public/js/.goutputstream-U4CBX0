                            var totalprice = price * nopint

                            var rounded_totalprice = totalprice.toFixed(2);

                            document.getElementById("pages_right").textContent = nop + " pages " + " x " + rounded_price;
                            document.getElementById("pages_sum").textContent = " USD " + rounded_totalprice;
							
							$('#aggpprice').val(rounded_totalprice);
							var sprice = $('#aggsprice').val();
							var spriceint = parseFloat(sprice);
							var totalpriceint = parseFloat(rounded_totalprice);
							var amount = totalpriceint + spriceint;
							$('#aggprice').val(amount);

                            $('#inputFieldnop').change(function () {

                                var nop = $(this).val();

                                var nopint = parseInt(nop);

